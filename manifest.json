{
    "name": "WCAG2.0&2.2",
    "version": "2.0",
    "description": "Web Accessibility Guidelines",
    "minimum_chrome_version": "46",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "http://localhost/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "js": [
                "AccessibleAuthentication_3_3_7(A).js",
                "Contrast(Enhanced)_1_4_6(AAA).js",
                "Contrast(Minimum)_1_4_3(AA).js",
                "Dragging_2_5_7(AA).js",
                "FixedReferencePoints_2_4_13(A).js",
                "FocusAppearanceEnhanced_2_4_12(AAA).js",
                "FocusAppearanceMinimum_2_4_11(AA).js",
                "HeadingsAndLabels_2_4_6(AA).js",
                "HiddenControls_3_2_7(AA).js",
                "InfoAndRelationships_1_3_1(A).js",
                "jquery-3.6.0.js",
                "Keyboard_2_1_1(A).js",
                "LabelsOrInstructions_3_3_2(A).js",
                "LanguageOfPage_3_1_1(A).js",
                "LinkPurpose_2_4_4(A).js",
                "manifest.json",
                "NonTextContent_1_1_1(A).js",
                "Parsing_4_1_1(A).js",
                "Pause,Stop,Hide_2_2_2(A).js",
                "ResizeText_1_4_4(AA).js",
                "Target Size_(Minimum)_2_5_8(AA).js",
                "UseOfColor_1_4_1(A).js"
            ],
            "all_frames": true
        }
    ],
    "manifest_version": 2
}